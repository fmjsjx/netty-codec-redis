<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn">
  <Appenders>
    <Console name="STDOUT" target="SYSTEM_OUT">
      <PatternLayout>
        <Pattern>%d{HH:mm:ss.SSS} [%t] %-5p %c:%L - %m%n</Pattern>
      </PatternLayout>
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="me.netty" level="trace">
      <AppenderRef ref="STDOUT" />
    </Logger>
    <Root level="off" />
  </Loggers>
</Configuration>